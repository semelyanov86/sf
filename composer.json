{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "akaunting/money": "^1.2",
        "bensampo/laravel-enum": "^3.2",
        "bugsnag/bugsnag-laravel": "^2.20",
        "darkaonline/l5-swagger": "^8.0",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "funeralzone/valueobjects": "^0.5.0",
        "garygreen/pretty-routes": "^1.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "laravel/framework": "^8.0",
        "laravel/passport": "^10.1",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "laraveldaily/laravel-charts": "^0.1.24",
        "nuovo/spreadsheet-reader": "^0.5.11",
        "predis/predis": "^1.1",
        "spatie/data-transfer-object": "^2.6",
        "spatie/laravel-fractal": "^5.8",
        "spatie/laravel-medialibrary": "^9.4.1",
        "spatie/laravel-query-builder": "^3.3",
        "spatie/laravel-ray": "^1.3",
        "spatie/laravel-tags": "^3.0",
        "spatie/laravel-view-models": "^1.3",
        "yajra/laravel-datatables-oracle": "^9.11"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "laravel/dusk": "^6.11",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.3",
        "psalm/plugin-laravel": "^1.4",
        "vimeo/psalm": "^4.3"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/Core",
            "Domains\\": "app/Domains",
            "Parents\\": "app/Parents",
            "Support\\": "app/Support",
            "Units\\": "app/Units",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
